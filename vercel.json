{
      "version": 2,
      "builds": [
        {
          "src": "api/index.js",
          "use": "@vercel/node"
        },
        {
          "src": "package.json",
          "use": "@vercel/static-build",
          "config": {
            "outputDirectory": "dist"
          }
        }
      ],
      "routes": [
        {
          "src": "/db-health",
          "dest": "api/index.js"
        },
        {
          "src": "/login",
          "dest": "api/index.js"
        },
        {
          "src": "/register",
          "dest": "api/index.js"
        },
        {
          "src": "/me",
          "dest": "api/index.js"
        },
        {
          "src": "/logout",
          "dest": "api/index.js"
        },
        {
          "src": "/(.*)",
          "dest": "api/index.js"
        }
      ]
    }