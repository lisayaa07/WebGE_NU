{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [

    {
      "src": "/(login|register|db-health|testtt|faculty|grades|subject-groups|subjects/.*|interestd|groupwork|solowork|exam|attendance|instruction|present|experience|challenge|time|submit-form|me|logout|cbr-match|grouped-subjects|favorites/.*|popular-subjects|students/.*)",
      "dest": "api/index.js"
    },

    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}